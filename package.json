    "type": "git",
    "url": "https://github.com/LunasaVT/GamploSDK.git"
  },
  "bugs": {
    "url": "https://github.com/LunasaVT/GamploSDK/issues"
  },
  "homepage": "https://github.com/LunasaVT/GamploSDK#readme",
